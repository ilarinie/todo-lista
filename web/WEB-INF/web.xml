<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Listaus</servlet-name>
        <servlet-class>todolist.servlets.Listaus</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KayttajaServlet</servlet-name>
        <servlet-class>todolist.servlets.KayttajaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TehtavaServlet</servlet-name>
        <servlet-class>todolist.servlets.TehtavaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewTehtavaServlet</servlet-name>
        <servlet-class>todolist.servlets.NewTehtavaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>todolist.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DestroyTehtavaServlet</servlet-name>
        <servlet-class>todolist.servlets.DestroyTehtava</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SuoritaTehtava</servlet-name>
        <servlet-class>todolist.servlets.SuoritaTehtava</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Listaus</servlet-name>
        <url-pattern>/Listaus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KayttajaServlet</servlet-name>
        <url-pattern>/kayttajat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TehtavaServlet</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewTehtavaServlet</servlet-name>
        <url-pattern>/newtehtava</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DestroyTehtavaServlet</servlet-name>
        <url-pattern>/DestroyTehtava</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SuoritaTehtava</servlet-name>
        <url-pattern>/SuoritaTehtava</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index</welcome-file>
    </welcome-file-list>
</web-app>
